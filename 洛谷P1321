#include<iostream>
#include<string>
using namespace std;
int main(){
    string str1;
    cin >> str1;
    int boycount = 0,girlcount = 0;
    for(int i = 0;i < str1.length();i ++){
        if(str1[i] == 'b' || str1[i + 1] == 'o' || str1[i + 2] == 'y') {
            boycount ++;
        }
        else if(str1[i] == 'g' || str1[i + 1] == 'i' || str1[i + 2] == 'r' || str1[i + 3] == 'l') {
            girlcount ++;
    }
    }
    cout << boycount << endl;
    cout << girlcount << endl;
}
